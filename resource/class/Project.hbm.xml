<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name = "com.ideas2it.project.model.Project" table = "Project">
    <id name = "projectId" column = "project_id" type = "int">  
     <generator class="increment">  </generator>
    </id>  

    <property name = "projectName" column = "project_name" type = "string"/>
   
    <property name = "projectBudget" column = "project_budget" type = "string"/>
   
    <property name = "projectTimeFrame" column = "project_timeframe" type = "string"/>
    
    <property name = "projectDescription" column = "project_description" type = "string"/>
   
    <property name = "status" column = "status" type = "boolean"/>
    
   <!--  <set name="employees" table="Employee_project"  lazy="false" cascade = "all" >
            <key column = "project_id"/>
             <many-to-many class = "com.ideas2it.employee.model.Employee"/>       
        </set>  -->
    </class>
</hibernate-mapping>
